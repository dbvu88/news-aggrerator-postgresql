<!-- Put your News Aggregator articles.erb code here -->
<h1> The News </h1>
<ul>
  <% @articles.each do |article| %>
    <li >
      <h4><%= article.title %></h4>
      <a href="<%= article.url %>">
        <%= article.url %>
      </a>
      <p><%= article.description %></p>
    </li>
  <% end %>
</ul>
